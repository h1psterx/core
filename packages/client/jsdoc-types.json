{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./lib",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|dist)"
  },
  "opts": {
    "template": "../../node_modules/tsd-jsdoc/dist",
    "encoding": "utf8",
    "destination": "console",
    "recurse": true,
    "verbose": false
  }
}
